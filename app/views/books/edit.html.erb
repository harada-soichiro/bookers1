<h1>Editing Book</h1>
<%= form_for(@book) do |f| %>
    <h4>tite</h4>
    <%= f.text_field :title, class:"book_title" %>
    <h4>body</h4>
    <%= f.text_area :body, class:"book_body" %>
    <%= f.submit 'Update book' %>
<% end %>
<%= link_to "show", book_path(@book), class:"show_#{@book}"  %>
<%= link_to "back", books_path, class:"back" %>